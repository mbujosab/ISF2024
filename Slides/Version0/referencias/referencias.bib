@article{Garcia-Ferrer:IJF-16-207,
title = "Forecasting \{OECD\} industrial turning points using unobserved components models with business survey data ",
journal = "International Journal of Forecasting ",
volume = "16",
number = "2",
pages = "207--227",
year = "2000",
MONTH = {Jun},
note = "",
issn = "0169-2070",
doi = "10.1016/S0169-2070(99)00049-7",
url = "http://www.sciencedirect.com/science/article/pii/S0169207099000497",
author = "Antonio García-Ferrer and Marcos Bujosa-Brun",
keywords = "Turning point forecasting, Unobserved components and qualitative survey data ",
abstract = "The approach followed in this paper stresses the importance of timing in signalling turning points. This is done in two stages; first a signal that a turning point is likely to occur, and later a statement of when it will occur. We use Young's trend derivative method, adding leading qualitative survey data. We find high coherence between its low frequency component and that of the corresponding economic variable. We study industrial production in six \{OECD\} countries with special emphasis on France and Spain. Inclusion of survey data improves forecast accuracy. "
}

